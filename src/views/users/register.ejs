  <!-- heade -->
  <%- include('../partials/head') %> 
  <body>
 <!-- header -->
    <%- include('../partials/header') %> 
    <main class="main-register">
      <div class="register">
        <h2>Register</h2>
        <form action="/action_page.php" method="post">
          <div class="userinput">
            <input type="text" placeholder="Enter your name" required />
            <i class="uil uil-user" id="icon1"></i>
          </div>
          <div class="userinput">
            <input type="text" placeholder="Enter your email" required />
            <i class="uil uil-envelope icon" id="icon2"></i>
          </div>
          <div class="userinput">
            <input type="password" placeholder="Create password" required />
            <i class="uil uil-lock icon" id="icon3"></i>
          </div>
          <div class="userinput">
            <input type="password" placeholder="Confirm password" required />
            <i class="uil uil-lock icon" id="icon4"></i>
            <i class="uil uil-eye-slash showHidePw" id="icon5"></i>
          </div>
          <div class="conditions">
            <input type="checkbox" />
            <h3>I accept all terms and conditions</h3>
          </div>
          <div class="userinput button">
            <input type="Submit" value="Register Now" />
          </div>
          <div class="text">
            <h3>Already have an account? <a href="/login">Login</a></h3>
          </div>
        </form>
        <p class="conditions">
          By registering to IT Land you agree to our terms and conditions and agree to our policies of comunication.
        </p>
      </div>
    </main>
    <!-- Bottom NAv -->
    <%- include('../partials/bottomNav') %> 
    <!-- Footer -->
    <%- include('../partials/footer') %> 
  </body>
</html>